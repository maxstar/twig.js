{
    "name": "jms/twig-js",
    "type": "library",
    "description": "twig.js, flexible, secure, and high-performance templating engine for Javascript",
    "keywords": ["templating"],
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "twig/twig": "^1.34.0@stable|^2.11"
    },
    "require-dev": {
        "dnode/dnode": "v0.2.0",
        "symfony/phpunit-bridge": "^4.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TwigJs\\": "src/TwigJs",
            "TwigJs\\Tests\\": "tests/TwigJs/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
